{
  "settings":{
    "number_of_shards":6,
    "number_of_replicas":1,
    "index":{
      "analysis":{
        "analyzer":{
          "default":{
            "tokenizer":"ik_max_word",
            "filter":[
              "asciifolding",
              "lowercase",
              "ourEnglishFilter"
            ]
          }
        },
        "filter":{
          "ourEnglishFilter":{
            "type":"kstem"
          }
        }
      }
    }
  },
  "mappings":{
    "_doc":{
      "properties":{
        "tid":{
          "type":"keyword"
        },
        "name":{
          "type":"text",
          "analyzer": "ik_max_word"
        },"describe":{
          "type":"text",
          "analyzer": "ik_max_word"
        },
        "createTime":{
          "type":"date"
        },
        "startTime":{
          "type":"date"
        },
        "endTime":{
          "type":"date"
        },
        "type":{
          "type":"keyword"
        },
        "processNum":{
          "type":"integer"
        },
        "uid":{
          "type":"keyword"
        },
        "isDelete":{
          "type":"integer"
        },
        "status":{
          "type":"integer"
        },
        "alarm":{
          "type":"integer"
        },
        "source":{
          "type":"text",
          "analyzer": "ik_max_word"
        }
      }
    }
  }
}